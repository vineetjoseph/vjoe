print ("Pick a number between one to one Million")
print("")
count = 1
while True:
  guess = input("What is your guess?:")
  if guess > "5":
    print ("too high")
    print("")
  elif guess < "5":
    print("too low")
    print("")
  else:
    print("you got that right")
    print("")
    break
  count += 1
print("")
print("It took you", count, "turns to guess it right")
